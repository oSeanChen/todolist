<h1>Categories#new</h1>
<p>Find me in app/views/categories/new.html.erb</p>
<% if @category.errors.full_messages.any? %>
  <div class="text-red-500">
    <ul>
      <% @category.errors.full_messages.each do |error_message| %>
        <li>
          <%= error_message %> <br />
        </li>
      <% end %>
    </ul>
  </div>
<% end %>
<%= form_with model:@category do |f| %>
  <%= f.label :title, "title" %>
  <%= f.text_field :title %>
  <%= f.submit "Submit"%>
<% end %>
